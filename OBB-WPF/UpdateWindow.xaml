<Window x:Class="OBB_WPF.UpdateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OBB_WPF"
        mc:Ignorable="d"
        Title="UpdateWindow" Height="450" Width="auto">
    <ListView>
        <TextBlock Text="{Binding Path=CurrentSeries}" HorizontalAlignment="Stretch" x:Name="CurrentSeries"></TextBlock>
        <ProgressBar Minimum="0" Maximum="0" Value="0" HorizontalAlignment="Stretch" x:Name="Progress"></ProgressBar>
    </ListView>
</Window>
