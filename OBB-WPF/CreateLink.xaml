<Window x:Class="OBB_WPF.CreateLink"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OBB_WPF"
        mc:Ignorable="d"
        Title="CreateLink" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox x:Name="Link" HorizontalAlignment="Stretch"></TextBox>
        <TextBox x:Name="Target" HorizontalAlignment="Stretch" Grid.Row="1"></TextBox>
        <Button Content="Save" Click="Button_Click" Grid.Row="2"></Button>
    </Grid>
</Window>
