<Window x:Class="OBB_WPF.Custom.CreateCustomSeries"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OBB_WPF.Custom"
        mc:Ignorable="d"
        Title="CreateCustomSeries" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Label Content="Series Name"></Label>
            <TextBox x:Name="SeriesName" MinWidth="100" HorizontalAlignment="Stretch"></TextBox>
        </StackPanel>
        <Button Content="Create" Grid.Row="1" Click="Button_Click"></Button>
    </Grid>
</Window>
